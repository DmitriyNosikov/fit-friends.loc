{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "server/src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "assets",
        "outDir": "./dist/server/src/",
        "watchAssets": true
      }
    ]
  }
}
